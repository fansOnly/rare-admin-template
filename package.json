{
  "name": "rare-admin-template",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "commit": "czg",
    "dev": "vite",
    "dev:mock": "cross-env MOCK=true npm run dev",
    "build": "vue-tsc --noEmit && vite build",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.json && pretty-quick --check --branch main",
    "lint:fix": "eslint --fix . --ext .vue,.js,.ts,.jsx,.json && pretty-quick --branch main",
    "preview": "vite preview",
    "report": "cross-env REPORT=true npm run build",
    "test": "vitest",
    "prepare": "husky install"
  },
  "dependencies": {
    "@vueuse/core": "^8.7.5",
    "axios": "^0.27.2",
    "element-plus": "^2.2.6",
    "pinia": "^2.0.14",
    "vue": "^3.2.25",
    "vue-router": "^4.0.16"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@iconify-json/ep": "^1.1.6",
    "@types/jsdom": "^20.0.1",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/md5": "^2.3.2",
    "@types/mockjs": "^1.0.6",
    "@types/node": "^18.0.0",
    "@types/nprogress": "^0.2.0",
    "@types/sass": "^1.43.1",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "@vitejs/plugin-legacy": "^1.8.2",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vue/test-utils": "^2.2.4",
    "cross-env": "^7.0.3",
    "cz-git": "^1.4.1",
    "czg": "^1.4.1",
    "dayjs": "^1.11.4",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^9.1.1",
    "esno": "^0.16.3",
    "fast-glob": "^3.2.12",
    "husky": "^8.0.2",
    "jsdom": "^20.0.3",
    "jsonc-eslint-parser": "^2.1.0",
    "jsonwebtoken": "^8.5.1",
    "lint-staged": "^13.1.0",
    "md5": "^2.3.0",
    "mockjs": "^1.1.0",
    "nprogress": "^0.2.0",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "rollup-plugin-visualizer": "^5.7.1",
    "sass": "^1.53.0",
    "typescript": "^4.5.4",
    "unocss": "^0.47.6",
    "unplugin-auto-import": "^0.9.3",
    "unplugin-element-plus": "^0.4.0",
    "unplugin-icons": "^0.14.7",
    "unplugin-vue-components": "^0.21.1",
    "unplugin-vue-define-options": "^1.0.0",
    "vite": "^2.9.9",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-inspect": "^0.6.0",
    "vite-plugin-mock": "^2.9.6",
    "vitest": "^0.25.3",
    "vue-tsc": "^0.34.7"
  },
  "engines": {
    "node": ">= 14.x",
    "npm": ">= 3.10.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-git"
    }
  },
  "lint-staged": {
    "*.{vue,js,ts,jsx,tsx,md,json}": [
      "eslint --fix",
      "prettier --write ."
    ]
  }
}
